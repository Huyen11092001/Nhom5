@model IEnumerable<QUANLYSINHVIEN.Models.Qlmh>

@{
    ViewData["Title"] = "Index";
}

<h1>Danh sách môn học</h1>

<p>
    <a asp-action="Create">Thêm môn học</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Mamonhoc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tenmonhoc)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Mamonhoc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tenmonhoc)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Mamonhoc">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Mamonhoc">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Mamonhoc">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
